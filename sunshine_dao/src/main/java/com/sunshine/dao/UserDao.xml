<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sunshine.dao.UserDao">
<select id="selectUserInfoForRegister" parameterType="com.sunshine.model.UserInfo" resultType="com.sunshine.model.UserInfo">
  select su.username, su.password from ss_user su where username = #{username} OR telephone = #{telephone}
</select>






























</mapper>